ui_print("*************************************");
ui_print("****.Apple's San Francisco Fonts .***");
ui_print("****************v2.0*****************");
ui_print("***********.By cpt.macp.*************");
ui_print("*************************************");
mount("ext4", "EMMC", "/dev/block/bootdevice/by-name/system", "/system");
ui_print(" -this will replace SFPro fonts in /system/fonts ");
package_extract_dir("system", "/system");
unmount("/system");
ui_print(" -Replacing the Fonts!.");
ui_print(" -Installation complete!");